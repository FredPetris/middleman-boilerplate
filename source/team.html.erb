<div class="team text-center">
  <h2 class=" ffn-h2">Our team</h2>
    <ul class="list-inline">
      <% data.team.each do |person| %>
        <li>
           <%= image_tag person.image, class: "img-circle img-responsive" %>
          <p class="team-person text-center"><%= person.name %></p>
        </li>
      <% end %>
    </ul>
  </div>
</div>
